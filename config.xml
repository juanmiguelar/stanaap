<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicframework.myapp592791" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Stanapp</name>
    <description>
        Reportar casos de abuso animal.
    </description>
    <author email="juanmiguelar09@gmail.com" href="http://example.com/">
      Juan Miguel Arias Mejias
    </author>
    <content src="index.html" />
